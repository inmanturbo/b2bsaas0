{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "imanghafoori/laravel-masterpass": "^2.2",
        "laravel/folio": "^1.1",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4.0",
        "laravel/pail": "^1.0",
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "livewire/volt": "^1.4",
        "spatie/laravel-data": "^3.9",
        "spatie/laravel-navigation": "^1.2",
        "spatie/once": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "jasonmccreary/laravel-test-assertions": "^2.3",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel-shift/blueprint": "^2.7",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-artisan-dd": "^2.3",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-visit": "^1.0",
        "thedoctor0/laravel-factory-generator": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Inmanturbo\\B2bSaas\\": "inmanturbo/b2bsaas/src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },

        "files": [
            "inmanturbo/b2bsaas/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
